import java.util.*;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        while(sc.hasNext()){
            String str = sc.nextLine();
            String[] s = str.split("[^a-zA-Z]+");  //以非a-z和非A-Z的字符为间隔符
            for(int i=s.length-1;i>=0;i--){
                System.out.print(s[i]+" ");
            }
        }
    }
}
